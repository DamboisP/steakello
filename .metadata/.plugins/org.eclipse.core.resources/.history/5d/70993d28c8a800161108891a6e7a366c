package model;

import util.Observable;
import util.Observer;

public class GameCore implements Observable{
	private Observer gameObserver;
	
	@Override
	public void notifyView(int gameMode, GameBoard gameBoard) {
		gameObserver.update(gameMode, gameBoard);
		
	}

	@Override
	public void addObserver(Observer obs) {
		this.gameObserver = obs;
		
	}
	
}
